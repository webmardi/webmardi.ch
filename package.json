    "babel-eslint": "^10.1.0",
    "babel-plugin-macros": "^3.1.0",
    "cypress": "^8.5.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-html": "^6.2.0",
