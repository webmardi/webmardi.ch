    "@types/node": "^20.11.0",
    "@types/ramda": "0.29.9",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@types/react-html-parser": "^2.0.6",
    "@types/react-mailchimp-subscribe": "^2.1.4",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "babel-eslint": "^10.1.0",
    "cypress": "^13.6.2",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
