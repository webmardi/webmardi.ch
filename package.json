    "postcss-import": "^16.0.0",
    "ramda": "^0.29.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-html-parser": "^2.0.2",
    "react-i18next": "^14.0.0",
    "react-indiana-drag-scroll": "^2.2.0",
    "react-mailchimp-subscribe": "^2.1.3",
